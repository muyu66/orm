{
    "name": "muyu/orm",
    "description": "A lightweight, high-performance ORM framework",
    "type": "library",
    "require": {
        "php": ">=5.5.9",
        "predis/predis": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "symfony/var-dumper": "^3.2"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Zhou Yu",
            "email": "muyu.zhouyu@outlook.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Orm\\": "src/"
        },
        "files": [
            "src/Commons/Function.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "TestCase\\": "tests/"
        }
    },
    "minimum-stability": "stable"
